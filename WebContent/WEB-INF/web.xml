<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <welcome-file-list>
  <welcome-file>
      Loginform.jsp
  </welcome-file>
  </welcome-file-list>
  <servlet>
     <servlet-name>First</servlet-name>
     <servlet-class>ServletsPackage.LoginServlets</servlet-class>
  </servlet>
  <servlet-mapping>
     <servlet-name>First</servlet-name>
     <url-pattern>/Login</url-pattern>
  </servlet-mapping>
  
  <servlet>
     <servlet-name>Second</servlet-name>
     <servlet-class>ServletsPackage.SignupServlet</servlet-class>
  </servlet>
  <servlet-mapping>
     <servlet-name>Second</servlet-name>
     <url-pattern>/Signup1</url-pattern>
  </servlet-mapping>
  
    <servlet>
     <servlet-name>Third</servlet-name>
     <servlet-class>ServletsPackage.DeleteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
     <servlet-name>Third</servlet-name>
     <url-pattern>/Delete</url-pattern>
  </servlet-mapping>
  
   
       
</web-app>