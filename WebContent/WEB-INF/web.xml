<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>WebProject</display-name>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>


 <servlet>
  	<servlet-name>login</servlet-name>    <!-- This is local name, just for mapping purpose only here -->
  	<servlet-class>com.testwebproject.LoginServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>login</servlet-name>   
  	<url-pattern>/loginsv</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
  	<servlet-name>signup</servlet-name>
  	<servlet-class>com.testwebproject.SignUpServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>signup</servlet-name>
  	<url-pattern>/signupsv</url-pattern>
  </servlet-mapping>
  
</web-app>